<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AccesoControllerServlet</servlet-name>
        <servlet-class>TP_Previo.Controllers.AccesoControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarZonaControllerServlet</servlet-name>
        <servlet-class>TP_Previo.Controllers.RegistrarZonaControllerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CambiarClaveControllerServlet</servlet-name>
        <servlet-class>TP_Previo.Controllers.CambiarClaveControllerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccesoControllerServlet</servlet-name>
        <url-pattern>/AccesoControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarZonaControllerServlet</servlet-name>
        <url-pattern>/RegistrarZonaControllerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CambiarClaveControllerServlet</servlet-name>
        <url-pattern>/CambiarClaveControllerServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
